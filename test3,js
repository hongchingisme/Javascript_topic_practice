function isUpperCase(ABC) {
  if (ABC.length == 0) {
    return false;
  }

  if (ABC[0] == ABC[0].toUpperCase()) {
    return true;
  } else {
    return false;
  }
}

console.log(isUpperCase("ABCD")); // returns true
console.log(isUpperCase("")); // returns false
console.log(isUpperCase("aBCD")); // returns false
